  pod 'gRPC', '1.11.0'
  pod 'gRPC-Core', '1.11.0'
  pod 'gRPC-ProtoRPC', '1.11.0'
  pod 'gRPC-RxLibrary', '1.11.0'

  target 'PocketCoreTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
